    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vite-plugin-chunk-split": "^0.4.7",
